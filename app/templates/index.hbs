  <tbody>
    {{#each model as |trial|}}
    <tr>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.acronym}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.armGroups}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.biospecDescr}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.biospecRetention}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.briefSummary}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.briefTitle}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.clinicalResults}}</p></td>
