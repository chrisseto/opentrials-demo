      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.source}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.sponsors}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.startDate}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.studyDesign}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.studyType}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.targetDuration}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.url}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.verificationDate}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.whyStopped}}</p></td>
    </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}
