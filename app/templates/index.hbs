      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.primaryCompletionDateActual}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.primaryCompletionDateAnticipated}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.primaryOutcomes}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.references}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.removedCountries}}</p></td>
