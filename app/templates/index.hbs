      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.interventions}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.isFdaRegulated}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.isSection801}}</p></td>
      <td class="mdl-data-table__cell--non-numeric">
	      <ul>
	      	{{#each trial.keywords as |keyword|}}
	      		<li>{{#link-to "trials.index" (query-params "filter[keywords]"=keyword) }}</li>
	      	{{/each}}
	      </ul>
      </td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.lastchangedDate}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.links}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.linkText}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.locationCountries}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.locations}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.nctAliases}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.nctId}}</p></td>
      <td><p>{{trial.numberOfArms}}</p></td>
      <td><p>{{trial.numberOfGroups}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.officialTitle}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.orgStudyId}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.otherOutcomes}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.overallContact}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.overallContactBackup}}</p></td>
      <td class="mdl-data-table__cell--non-numeric"><p>{{trial.overallOfficials}}</p></td>
